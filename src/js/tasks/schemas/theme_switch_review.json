{
    "type": "object",
    "properties": {
      "themeName": {
        "type": "string",
        "example": "My Custom Theme"
      },
      "switchDate": {
        "type": "string",
        "format": "date-time",
        "example": "2025-05-07T10:00:00Z"
      },
      "userId": {
        "type": "string",
        "example": "usr_12345"
      },
      "review": {
        "type": "string",
        "example": "The new theme looks great and is easy to navigate!"
      },
      "rating": {
        "type": "integer",
        "minimum": 1,
        "maximum": 5,
        "example": 4
      },
      "suggestions": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "example": [
          "Add more color options",
          "Improve loading speed"
        ]
      }
    },
    "required": [
      "themeName",
      "switchDate",
      "userId",
      "review"
    ]
}