{
  "manifest_version": 3,
  "name": "Headless CMS Suite",
  "version": "1.0.0",
  "description": "Headless cms suite browser extension made for wordpress multi-purpose.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "dist/icons/brand/favicon-32x32.png"
  },
  "permissions": [
    "storage",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bot/socket.io.min.js", "dist/js/schemaeditor.js"],
      "css": ["styling.css"]
    }
  ],
  "icons": {
    "32": "dist/icons/brand/favicon-32x32.png",
    "192": "dist/icons/brand/android-chrome-192x192.png",
    "512": "dist/icons/brand/android-chrome-512x512.png"
  },
  "notification": {
    "default_title": "New Form Submission",
    "default_icon": "dist/icons/brand/favicon-32x32.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["socket.io.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}