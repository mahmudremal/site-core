{
  "manifest_version": 3,
  "name": "Dom Selector Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["socket.io.min.js", "dist/js/schemaeditor.js"],
      "css": ["styling.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["socket.io.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
