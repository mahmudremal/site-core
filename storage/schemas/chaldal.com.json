{
  "wait4selection": [],
  "removals": {
    "header": ".headerWrapper",
    "footer": "#footer",
    "sidebar": "#rhf, .similarities-widget",
    "ads": ".shoppingCartWrapper, .menuWrapper"
  },
  "extract": {
    "isProduct": "#module_item_gallery_1 > .item-gallery",
    "product": {
      "wait4selection": [],
      "content": [".productDetails section.right .details", "innerText"],
      "images": [".productGallery img", "src"],
      "price": [".quantityContainer .discountedPriceSection .discountedPrice span[itemprop=price]", "content"],
      "currency": [".quantityContainer .discountedPriceSection .discountedPrice span[itemprop=priceCurrency]", "content"],
      "originalPrice": [".quantityContainer .fullPrice span + span", "innerText"],
      "discount": [".quantityContainer .discount span", "innerText"],
      "title": [".productDetails section.right div.nameAndSubtext h1[itemprop=name]", "innerText"],
      "rating": [],
      "availability": [".quantityContainer link[itemprop=availability]", "href"],
      "brand": [],
      "category": [],
      "specifications": [],
      "reviews": {
        "selector": [],
        "count": [],
        "items": []
      },
      "meta": {
        "title": [".productDetails section.right div.nameAndSubtext h1[itemprop=name]", "innerText"],
        "description": ["meta[name='description']", "content"],
        "ogImage": ["meta[property='og:image']", "content"],
        "ogTitle": ["meta[property='og:title']", "content"],
        "ogDescription": ["meta[property='og:description']", "content"],
        "ogPrice": ["meta[property='product:price:amount']", "content"],
        "ogCurrency": ["meta[property='product:price:currency']", "content"]
      },
      "custom": {
        "seller": {
          "name": [],
          "url": [],
          "info": []
        },
        "warranty": [],
        "delivery": []
      }
    },
    "isCategory": ".catalogBrowser",
    "category": {
      "wait4selection": [".catalogBrowser"],
      "title": [".categoryHeader .breadcrumb a span[itemprop=name]", "innerText"],
      "meta": {
        "title": [".categoryHeader .breadcrumb a span[itemprop=name]", "innerText"],
        "description": ["meta[name='description']", "content"],
        "ogImage": ["meta[property='og:image']", "content"],
        "ogTitle": ["meta[property='og:title']", "content"],
        "ogDescription": ["meta[property='og:description']", "content"]
      }
    },
    "links": ["[data-qa-locator='general-products'] [data-qa-locator='product-item'] a[title], a.product-item-link, .ant-pagination .ant-pagination-item:not(.ant-pagination-item-active) > a, a[href^='https://www.daraz.com.bd/products/']", "href", "^(https?:\\/\\/[^/]+\\/products\\/[^.]+\\.html)"]
  }
}
