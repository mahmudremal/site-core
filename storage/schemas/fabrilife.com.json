{
  "removals": {
    "header": "header, .header, nav, .navigation",
    "footer": "footer, .footer",
    "sidebar": ".sidebar, .ads, .advertisement",
    "popup": ".popup, .modal, .overlay"
  },
  "extract": {
    "isProduct": ".pdp-product-detail, .product-detail, [data-testid='product-detail']",
    "product": {
      "content": ".product-description, .pdp-product-desc",
      "images": ".gallery-preview-panel img, .product-images img, .pdp-gallery img",
      "price": ".price-current, .pdp-price .price",
      "originalPrice": ".price-original, .pdp-price .original-price",
      "discount": ".discount-percent, .pdp-price .discount",
      "title": "h1.pdp-mod-product-badge-title, .product-title h1",
      "rating": ".score-average, .rating-score",
      "availability": ".delivery-info, .stock-info",
      "brand": ".pdp-product-brand, .brand-name",
      "category": ".breadcrumb a",
      "specifications": ".specification-table tr, .product-specs .spec-item",
      "reviews": {
        "selector": ".reviews-container, .product-reviews",
        "count": ".reviews-count, .total-reviews",
        "items": ".review-item, .single-review"
      },
      "meta": {
        "title": "title",
        "description": "meta[name='description']",
        "ogImage": "meta[property='og:image']",
        "ogTitle": "meta[property='og:title']",
        "ogDescription": "meta[property='og:description']",
        "ogPrice": "meta[property='product:price:amount']",
        "ogCurrency": "meta[property='product:price:currency']"
      },
      "custom": {
        "seller": ".seller-info .seller-name, .pdp-seller-name",
        "warranty": ".warranty-info, .product-warranty",
        "delivery": ".delivery-info, .shipping-info"
      }
    },
    "isCategory": ".category-products, .search-results, .category-listing",
    "category": {
      "products": ".grid-item, .product-item, .category-product",
      "productTitle": ".product-title, .item-title",
      "productPrice": ".price, .item-price",
      "pagination": ".pagination a, .next-page",
      "filters": ".filters, .category-filters",
      "sorting": ".sort-options, .sorting",
      "meta": {
        "title": "title",
        "description": "meta[name='description']",
        "ogImage": "meta[property='og:image']",
        "totalProducts": ".results-count, .total-items"
      }
    },
    "links": "a[href*='/products/'], a[href*='/category/'], .product-item a, .category-link a"
  }
}